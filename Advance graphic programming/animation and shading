import maya.cmds as sb
sb.file(f=True,newFile=True)

sb.polyPlane(w=100,h=100)
sb.ambientLight(n='AL')
sb.move(0,5,0)

sb.polyCube(n='myCube')
sb.move(0,0.5,0)

sb.select('AL')
sb.ambientLight(n='AL',e=True,rgb=[1,1,0])

sb.spotLight(n='SL',i=1.5,p=10,do=0.5,rgb=(1,0,1))
sb.move(0,8,0)
sb.rotate(-90,0,0)

