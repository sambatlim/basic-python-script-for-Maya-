import maya.cmds as sb
myPath=sb.curve(d=2,p=[(0,0,0),(0,5,0),(5,5,0),(5,0,0),(0,0,0)],bez=True)
myCube=sb.polyCone()
sb.pathAnimation(myCube[0],c=myPath,stu=0,etu=200,f=True)

