----------------------------------------

import maya.cmds as sb
sb.file (f=1,newFile=1)

for i in range(160):
    mycl=sb.polyCylinder(n='mycube%d'%i,r=0.5)
    sb.move(i,0,0)
    
    sb.expression(o=mycl[0],s='ty=25*time*pow(cos(tx/10)*time,2)')
    sb.expression(o=mycl[1],s='h=30')
    
